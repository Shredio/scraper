{
	"name": "shredio/scraper",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.4",
		"symfony/http-client": "^7.0"
	},
	"autoload": {
		"psr-4": {
			"Shredio\\Scraper\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"test": "vendor/bin/phpunit tests"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"phpunit/phpunit": "^12.3",
		"symfony/dependency-injection": "^7.3",
		"symfony/http-kernel": "^7.3",
		"symfony/config": "^7.3"
	}
}
